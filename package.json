{
  "name": "vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "pretty:fix": "prettier --config ./.prettierrc --write \"src/**/*.{ts,vue,scss}\"",
    "pretty:check": "prettier --config ./.prettierrc --check \"src/**/*.{ts,vue,scss}\"",
    "lint:check": "eslint -c ./.eslintrc.cjs \"src/**/*.{ts,vue}\"",
    "lint:fix": "eslint --fix -c ./.eslintrc.cjs \"src/**/*.{ts,vue}\""
  },
  "dependencies": {
    "axios": "^1.7.9",
    "element-plus": "^2.9.8",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.0.1",
    "vite-plugin-checker": "^0.9.1",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.1.10"
  }
}
